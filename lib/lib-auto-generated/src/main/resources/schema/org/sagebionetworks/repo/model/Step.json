{
	"title": "Step",
	"description": "This object is deprecated and will be removed in future versions of Synapse.",
	"implements": [
		{
			"$ref": "org.sagebionetworks.repo.model.Entity"
		}
	],
	"properties": {
		"code": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "org.sagebionetworks.repo.model.Reference"
			},
			"description": "References to Code",
			"title": "Code"
		},
		"input": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "org.sagebionetworks.repo.model.Reference"
			},
			"description": "References to Input",
			"title": "Input"
		},
		"output": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"$ref": "org.sagebionetworks.repo.model.Reference"
			},
			"description": "References to Output",
			"title": "Output"
		},
		"environmentDescriptors": {
			"type": "array",
			"uniqueItems": true,
			"contentEncoding": "binary",
			"items": {
				"$ref": "org.sagebionetworks.repo.model.EnvironmentDescriptor"
			},
			"description": "The list of environment descriptors",
			"title": "Environment Descriptors"
		},
		"startDate": {
			"type": "string",
			"format": "date-time",
			"description": "The start date of this Step",
			"title": "Start Date"
		},
		"endDate": {
			"type": "string",
			"format": "date-time",
			"description": "The end date of this Step",
			"title": "End Date"
		},
		"commandLine": {
			"type": "string",
			"description": "The end date of this Step",
			"title": "Command Line"
		}
	}
}