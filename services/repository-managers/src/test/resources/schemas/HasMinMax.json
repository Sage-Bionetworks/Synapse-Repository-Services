{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "hasMinMax",
	"description": "test for minimum and maximum",
	"properties": {
		"lessThanOrEqualsToTen": {
			"type": "integer",
			"maximum": 10
		},
		"greaterThanOrEqualToTwenty": {
			"type": "integer",
			"minimum": 20
		},
		"betweenThirtyAndForty": {
			"type": "integer",
			"minimum": 30,
			"maximum": 40
		}
	}
}
